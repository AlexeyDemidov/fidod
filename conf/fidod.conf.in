# fidod will switch to non-root user after startup
#
user 	@ifmailuser@

# period for checking inbound dir
checkperiod 10

facility @logfacility@
logfile	\
    @logfile@

unpack	@unpackprogram@
pack	@packprogram@

prot_inbound 	@protinbound@
inbound		@inbound@

pidfile @pidfile@

verbose no
